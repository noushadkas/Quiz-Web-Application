<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.user.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignIn</servlet-name>
        <servlet-class>com.user.SignIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadQuiz</servlet-name>
        <servlet-class>com.user.UploadQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentRegister</servlet-name>
        <servlet-class>com.user.StudentRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Generate_Certificate</servlet-name>
        <servlet-class>com.user.Generate_Certificate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignIn</servlet-name>
        <url-pattern>/SignIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadQuiz</servlet-name>
        <url-pattern>/UploadQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentRegister</servlet-name>
        <url-pattern>/StudentRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Generate_Certificate</servlet-name>
        <url-pattern>/Generate_Certificate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
